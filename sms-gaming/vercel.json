{
  "version": 2,
  "builds": [
    {
      "src": "api/serverless.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/serverless.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/serverless.js"
    }
  ],
  "buildCommand": "npm install"
} 